<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="ArthookGen.Infraestructure"
		                   namespace="ArthookGen.Infraestructure.EN.Arthook">
		    
		    
			
				<class name="UsuarioNH" table="Usuario"> 
				  	
						<id name="Id" column="idUsuario" type="integer"> 
							<generator class="hilo"/>
						</id>
			
					
					<property name="Nombre" type="string"> 
						<column name="nombre"  not-null="true"/>
					</property>
					<property name="Email" type="string"> 
						<column name="email"  not-null="true"/>
					</property>
					<property name="Nickname" type="string"> 
						<column name="nickname"  not-null="true"/>
					</property>
					<property name="Telefono" type="string"> 
						<column name="telefono"  not-null="true"/>
					</property>
					<property name="TipoUsuario" > 
						<column name="tipoUsuario"  not-null="true"/>
					</property>
					<property name="ValoracionMedia" type="double"> 
						<column name="valoracionMedia"  not-null="true"/>
					</property>
					<property name="Pass" type="string"> 
						<column name="pass"  not-null="true"/>
					</property>
			
			
			
			
			
			
			
		  			
						<bag name="MensajeE" >
							<key>
							
								
								<column name="FK_idUsuario_idEmisor"/>
					      	</key>
							<one-to-many class="ArthookGen.Infraestructure.EN.Arthook.MensajeNH"/>
					    </bag>
					
			
		  			
						<bag name="MensajeR" >
							<key>
							
								
								<column name="FK_idUsuario_idReceptor"/>
					      	</key>
							<one-to-many class="ArthookGen.Infraestructure.EN.Arthook.MensajeNH"/>
					    </bag>
					
			
		  			
						<bag name="Publicacion" >
							<key>
							
								
								<column name="FK_idUsuario_idUsuario"/>
					      	</key>
							<one-to-many class="ArthookGen.Infraestructure.EN.Arthook.PublicacionNH"/>
					    </bag>
					
			
		  			
						<bag name="Tarifa" >
							<key>
							
								
								<column name="FK_idUsuario_idUsuario"/>
					      	</key>
							<one-to-many class="ArthookGen.Infraestructure.EN.Arthook.TarifaNH"/>
					    </bag>
					
			
		  			
						<bag name="Valoracion" >
							<key>
							
								
								<column name="FK_idUsuario_idUsuario"/>
					      	</key>
							<one-to-many class="ArthookGen.Infraestructure.EN.Arthook.ValoracionPublicacionNH"/>
					    </bag>
					
			
		  			
						<bag name="Pedido" >
							<key>
							
								
								<column name="FK_idUsuario_idUsuario"/>
					      	</key>
							<one-to-many class="ArthookGen.Infraestructure.EN.Arthook.PedidoNH"/>
					    </bag>
					
			
		  			
						<bag name="Devolucion" >
							<key>
							
								
								<column name="FK_idUsuario_idUsuario"/>
					      	</key>
							<one-to-many class="ArthookGen.Infraestructure.EN.Arthook.DevolucionNH"/>
					    </bag>
					
			
			
		  			
						<bag name="MetodoPago" table="[usuario_metodoPago]"  >
							<key>
								
									<column name="FK_idUsuario_idUsuario"/> 
							</key>
      						<many-to-many class="ArthookGen.Infraestructure.EN.Arthook.MetodoPagoNH">
									<column name="FK_idMetodoPago_idMetodoPago"/> 
      						</many-to-many>
    					</bag>
			
			
		  			
						<bag name="ValoracionEmitidas" >
							<key>
							
								
								<column name="FK_idUsuario_idUsuarioValorador"/>
					      	</key>
							<one-to-many class="ArthookGen.Infraestructure.EN.Arthook.ValoracionUsuarioNH"/>
					    </bag>
					
			
		  			
						<bag name="ValoracionRecibidas" >
							<key>
							
								
								<column name="FK_idUsuario_idUsuarioValorado"/>
					      	</key>
							<one-to-many class="ArthookGen.Infraestructure.EN.Arthook.ValoracionUsuarioNH"/>
					    </bag>
					
			
					
					
					
					
					
					
					
					
					
					
					
					
					
					
					
					
					
					
		
		 </class>	
		  
		  <query name="UsuarioNHReadAllHQL">from UsuarioNH</query>
		</hibernate-mapping>
	