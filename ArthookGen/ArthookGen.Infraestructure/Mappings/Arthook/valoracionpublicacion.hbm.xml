<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="ArthookGen.Infraestructure"
		                   namespace="ArthookGen.Infraestructure.EN.Arthook">
		    
		    
			
				<class name="ValoracionPublicacionNH" table="ValoracionPublicacion"> 
				  	
						<id name="Id" column="idValoracionPublicacion" type="integer"> 
							<generator class="hilo"/>
						</id>
			
					
					<property name="Puntuacion" type="float"> 
						<column name="Puntuacion"  not-null="true"/>
					</property>
					<property name="Comentario" type="string"> 
						<column name="Comentario"  not-null="true"/>
					</property>
			
			
			
			
			
		  			
						<many-to-one name="Publicacion" class="ArthookGen.Infraestructure.EN.Arthook.PublicacionNH" >	
								<column name="FK_idPublicacion_idPublicacion"/> 
						</many-to-one>
											 					
			
		  			
						<many-to-one name="Usuario" class="ArthookGen.Infraestructure.EN.Arthook.UsuarioNH" >	
								<column name="FK_idUsuario_idUsuario"/> 
						</many-to-one>
											 					
					
					
					
					
					
		
		 </class>	
		  
		  <query name="ValoracionPublicacionNHReadAllHQL">from ValoracionPublicacionNH</query>
		</hibernate-mapping>
	