<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="ArthookGen.Infraestructure"
		                   namespace="ArthookGen.Infraestructure.EN.Arthook">
		    
		    
			
				<class name="TarifaNH" table="Tarifa"> 
				  	
						<id name="Id" column="idTarifa" type="integer"> 
							<generator class="hilo"/>
						</id>
			
					
					<property name="Precio" type="float"> 
						<column name="precio"  not-null="true"/>
					</property>
					<property name="Descripcion" type="string"> 
						<column name="descripcion"  not-null="true"/>
					</property>
					<property name="Nombre" type="string"> 
						<column name="nombre"  not-null="true"/>
					</property>
			
			
			
			
			
			
		  			
						<many-to-one name="UsuarioTarifa" class="ArthookGen.Infraestructure.EN.Arthook.UsuarioNH" >	
								<column name="FK_idUsuario_idUsuario"/> 
						</many-to-one>
											 					
					
					
					
					
					
		
		 </class>	
		  
		  <query name="TarifaNHReadAllHQL">from TarifaNH</query>
		</hibernate-mapping>
	