<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="ArthookGen.Infraestructure"
		                   namespace="ArthookGen.Infraestructure.EN.Arthook">
		    
		    
			
				<class name="DevolucionNH" table="Devolucion"> 
				  	
						<id name="Id" column="idDevolucion" type="integer"> 
							<generator class="hilo"/>
						</id>
			
					
					<property name="Descripcion" type="string"> 
						<column name="descripcion"  not-null="true"/>
					</property>
					<property name="Motivo" > 
						<column name="motivo"  not-null="true"/>
					</property>
					<property name="Fecha" type="date"> 
						<column name="fecha"  not-null="true"/>
					</property>
			
			
			
			
			
			
		  			
						<many-to-one name="UsuarioDevolucion" class="ArthookGen.Infraestructure.EN.Arthook.UsuarioNH" >	
								<column name="FK_idUsuario_idUsuario"/> 
						</many-to-one>
											 					
			
		  			
					
							<many-to-one name="PedidoDevolucion" class="ArthookGen.Infraestructure.EN.Arthook.PedidoNH" >
								<column name="FK_idPedido_idPedidoDevolucion"/> 
        					</many-to-one>
					 
						
					
					
					
					
					
					
		
		 </class>	
		  
		  <query name="DevolucionNHReadAllHQL">from DevolucionNH</query>
		</hibernate-mapping>
	