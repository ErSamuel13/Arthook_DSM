<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="ArthookGen.Infraestructure"
		                   namespace="ArthookGen.Infraestructure.EN.Arthook">
		    
		    
			
				<class name="ValoracionUsuarioNH" table="ValoracionUsuario"> 
				  	
						<id name="Id" column="idValoracionUsuario" type="integer"> 
							<generator class="hilo"/>
						</id>
			
					
					<property name="Puntuacion" type="string"> 
						<column name="puntuacion"  not-null="true"/>
					</property>
			
			
			
			
		  			
						<many-to-one name="Valorador" class="ArthookGen.Infraestructure.EN.Arthook.UsuarioNH" >	
								<column name="FK_idUsuario_idUsuario"/> 
						</many-to-one>
											 					
			
		  			
						<many-to-one name="Valorado" class="ArthookGen.Infraestructure.EN.Arthook.UsuarioNH" >	
								<column name="FK_idUsuario_idUsuario"/> 
						</many-to-one>
											 					
					
					
					
					
		
		 </class>	
		  
		  <query name="ValoracionUsuarioNHReadAllHQL">from ValoracionUsuarioNH</query>
		</hibernate-mapping>
	