<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="ArthookGen.Infraestructure"
		                   namespace="ArthookGen.Infraestructure.EN.Arthook">
		    
		    
			
				<class name="MetodoPagoNH" table="MetodoPago"> 
				  	
						<id name="Id" column="idMetodoPago" type="integer"> 
							<generator class="hilo"/>
						</id>
			
					
			
			
			
		  			
						<bag name="Pedido" >
							<key>
							
								
								<column name="FK_idMetodoPago_idMetodoPago"/>
					      	</key>
							<one-to-many class="ArthookGen.Infraestructure.EN.Arthook.PedidoNH"/>
					    </bag>
					
			
		  			
						<bag name="Usuario" table="[usuario_metodoPago]" inverse="true" >
							<key>
								
									<column name="FK_idMetodoPago_idMetodoPago"/> 
							</key>
      						<many-to-many class="ArthookGen.Infraestructure.EN.Arthook.UsuarioNH">
									<column name="FK_idUsuario_idUsuario"/> 
      						</many-to-many>
    					</bag>
					
					
					
		
		 </class>	
		  
		  <query name="MetodoPagoNHReadAllHQL">from MetodoPagoNH</query>
		</hibernate-mapping>
	